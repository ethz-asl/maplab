<?xml version="1.0" encoding="UTF-8"?>
<package format="2">
	<name>maplab_node</name>
	<version>2.2.0</version>
	<description>
		ROS node wrapping the mapping and localization
		capabilities of the maplab backend. It takes an odometry estimates
		(T_O_B: odometry to sensor-base) and raw sensor data (vision, imu,
		lidar) as input and constructs a pose-graph in order to enable
		optimization and visual/lidar-based localization.
	</description>
	<maintainer email="maplab-dev@mavt.ethz.ch">maplab-developers</maintainer>
	<license>Apache 2.0</license>

	<buildtool_depend>catkin_simple</buildtool_depend>
	<buildtool_depend>catkin</buildtool_depend>

	<depend>aslam_cv_cameras</depend>
	<depend>aslam_cv_common</depend>
	<depend>aslam_cv_frames</depend>
	<depend>aslam_cv_pipeline</depend>
	<depend>cv_bridge</depend>
	<depend>end_to_end_common</depend>
	<depend>feature_tracking</depend>
	<depend>gflags_catkin</depend>
	<depend>glog_catkin</depend>
	<depend>localization_fusion</depend>
	<depend>localization_summary_map</depend>
	<depend>loop_closure_handler</depend>
	<depend>map_sparsification_plugin</depend>
	<depend>maplab_common</depend>
	<depend>maplab_msgs</depend>
	<depend>maplab_ros_common</depend>
	<depend>maplab_test_data</depend>
	<depend>mapping_workflows_plugin</depend>
	<depend>message_flow</depend>
	<depend>minkindr_conversions</depend>
	<depend>online_map_builders</depend>
	<depend>sensors</depend>
	<depend>vi_map</depend>
	<depend>vio_common</depend>
	<depend>visualization</depend>

	<test_depend>vi_mapping_test_app</test_depend>
</package>
