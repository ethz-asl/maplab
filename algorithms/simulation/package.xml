<?xml version="1.0" encoding="UTF-8"?>
<package format="2">
  <name>simulation</name>
  <version>0.0.0</version>
  <description>The simulation package, providing ground-truth trajectory data together with simulated 6DOF IMU readings and visual features.</description>
  <maintainer email="maplab-dev@mavt.ethz.ch">maplab-developers</maintainer>
  <license>Apache 2.0</license>

  <buildtool_depend>catkin_simple</buildtool_depend>
  <buildtool_depend>catkin</buildtool_depend>

  <depend>aslam_cv_cameras</depend>
  <depend>aslam_cv_common</depend>
  <depend>aslam_cv_frames</depend>
  <depend>eigen_catkin</depend>
  <depend>eigen_checks</depend>
  <depend>geometry_msgs</depend>
  <depend>gflags_catkin</depend>
  <depend>glog_catkin</depend>
  <depend>maplab_common</depend>
  <depend>maplab_test_data</depend>
  <depend>minkindr_conversions</depend>
  <depend>posegraph</depend>
  <depend>protobuf_catkin</depend>
  <depend>roscpp</depend>
  <depend>roslib</depend>
  <depend>sensors</depend>
  <depend>std_msgs</depend>
  <depend>vi_map</depend>
  <depend>visualization_msgs</depend>
</package>
